{
    "name": "marcin-jozwikowski/easy-admin-pretty-urls",
    "description": "Semi-automatic URL generator for pretty REST-like URLs in EasyAdmin",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "easycorp/easyadmin-bundle": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "MarcinJozwikowski\\EasyAdminPrettyUrls\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcinJozwikowski\\EasyAdminPrettyUrls\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Jóźwikowski",
            "email": "marcin@jozwikowski.pl"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.9",
        "friendsofphp/php-cs-fixer": "^3.13"
    }
}
